<%= form_with model: @user, url: users_path do |f|%>
  <%= f.label "Username:"%>
  <%= f.text_field :username, :value => "test" %>

  <%= f.label "Email:" %>
  <%= f.text_field :email, :value => "test@example.com" %>

  <%= f.label "Password:" %>
  <%= f.text_field :password %>

  <%= f.submit "Create" %>
<% end %>

<%= link_to "Home" root_path %>